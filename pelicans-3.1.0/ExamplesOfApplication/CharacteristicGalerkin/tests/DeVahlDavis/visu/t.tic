BATCH
ECHO 0 STAT PS

FRAME WHITE 21. 21.

FILE 'save.gene' FORM END

CYCLE ALL
PHYS TIME TABLE TIME
ANAL TIME
   nCycle = NUMBER TIME
END-ANAL 

CYCLE 1

CALL "build_meshing.tic"

CYCLE (nCycle)

PHYS 0 T TABL T
PHYS TIME TABL TIME

FIELD NODE T
PALETTE
  0.2 0.2 1.0  20
  0.2 1.0 0.2  50
  1.0 1.0 0.0  80
  1.0 0.2 0.2 110
TERM
CUTT 5 AXIS X1
CUTT 5 AXIS Y1

ANAL TIME
  format="(' time = ',1PE12.5)"
  PRINT CHAR(TIME:1,format)
END-ANAL

PSFN "doc/t.ps"
DRAW PALETTE 0. 0. 10

