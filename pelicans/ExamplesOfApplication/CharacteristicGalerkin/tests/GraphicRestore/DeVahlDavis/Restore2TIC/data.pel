#include ( join( "..", "ref.pel" ) )

MODULE PEL_Application

   MODULE PDE_DomainAndFields
      
      MODULE GE_Meshing        
         concrete_name = "GE_BoxWithBoxes"        
         vertices_coordinate_1 = regular_vector( 0.0, 15, 2.0 )
         vertices_coordinate_0 = regular_vector( 0.0, 15, 1.0 )
         mesh_polyhedron = < "GE_Segment" "GE_Rectangle" >        
      END MODULE GE_Meshing
      
      // Read the initial field values :
      MODULE interior_fields
         MODULE velocity
            MODULE DOFs_values
               type = "defined_by_PDE_ResultReader"
               out_of_stored_grid_value = < 0.0 0.0 >
            END MODULE DOFs_values
         END MODULE velocity
         MODULE pressure
            MODULE DOFs_values
               type = "defined_by_PDE_ResultReader"
               out_of_stored_grid_value = < 0.0 >
            END MODULE DOFs_values
         END MODULE pressure
         MODULE temperature
            MODULE DOFs_values
               type = "defined_by_PDE_ResultReader"
               out_of_stored_grid_value = < 0.0 >
            END MODULE DOFs_values
         END MODULE temperature
      END MODULE interior_fields
      
      MODULE PDE_ResultReader
         MODULE PEL_DataOnMeshingReader
            concrete_name = "PEL_TICreader"
            files_basename = join( "..", "DoSaving", "save" )
            cycle = 3
         END MODULE PEL_DataOnMeshingReader
         MODULE fields
            MODULE velocity
               entry_name = "V"
               field = "velocity"
            END MODULE velocity
            MODULE pressure
               entry_name = "P"
               field = "pressure"
            END MODULE pressure
            MODULE temperature
               entry_name = "T"
               field = "temperature"
            END MODULE temperature
         END MODULE fields
      END MODULE PDE_ResultReader
 
   END MODULE PDE_DomainAndFields
   
   MODULE FE_TimeIterator
      time_initial = 0.
      time_end = 0.002
      time_step = 0.001
   END MODULE FE_TimeIterator

END MODULE PEL_Application
