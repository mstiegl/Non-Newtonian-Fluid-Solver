MODULE PEL_Application
   
   concrete_name = "FE_StepByStepProgression"

   $DS_YOUNG = 1.0985E+6
   $DS_POISSON = 0.3
   $DS_Force = 62.5E+3

   MODULE FE_TimeIterator
      time_initial = 0.0
      time_end = 1.
      time_step = 0.1
   END MODULE FE_TimeIterator

   graphics_output_times = regular_vector( 0., 2, 1. )
   
   MODULE PDE_DomainAndFields
      
      verbose_level = 1
      nb_space_dimensions = 2
      type = "finite_element"
      
      MODULE GE_Meshing
         concrete_name = "GE_GambitMeshing"
         mesh_polyhedron = < "GE_Segment" "GE_Quadrilateral" >
         filename = join( this_file_dir(), "cook.neu" )
      END MODULE GE_Meshing
      
      MODULE interior_fields
         
         MODULE uu
            name = "uu"
            nb_components = 2
            element_name = "PDE_2D_Q1_4nodes"
            storage_depth = 1
            MODULE DOFs_values
               type = "uniformly_defined"
               value = vector( 0.0, 0.0 )
            END MODULE DOFs_values
            MODULE DOFs_imposed_value
               MODULE west
                  location = "on_bounds"
                  type = "uniformly_defined"
                  color = "west_face"
                  value = < 0.0 0.0 >
               END MODULE west
            END MODULE DOFs_imposed_value
         END MODULE uu
         
      END MODULE interior_fields
      
      MODULE boundary_conditions
         
         MODULE uu
            field = "uu"
            color = "east_face"
            type = "Neumann"
         END MODULE uu
         
      END MODULE boundary_conditions
      
      MODULE PDE_ResultSaver
         
         writers = < "PEL_TICwriter" >
         
         writing_mode = "text"
         files_basename = "save"
         MODULE uu
            where_to_save = "at_vertices"
            entry_name = "UU"
            field = "uu"
         END MODULE uu
         
      END MODULE PDE_ResultSaver
      
   END MODULE PDE_DomainAndFields

   MODULE FE_SetOfParameters
      MODULE FE_Parameter#bend
         concrete_name = "FE_SpaceTimeParameter"
         name = "bend"
         nb_components = 2
         value = vector( 0.0, $DS_Force * $DS_T )
       END MODULE FE_Parameter#bend
       no_source = < 0.0 0.0 >
   END MODULE FE_SetOfParameters

   MODULE FE_OneStepIteration

      concrete_name = "FE_SplitSystem"

      MODULE list_of_FE_OneStepIteration

         MODULE FE_OneStepIteration#1
            concrete_name = "FE_BCupdate"
            field = "uu"
            levels_of_field = < 0 >
         END MODULE FE_OneStepIteration#1

         MODULE FE_OneStepIteration#2

            concrete_name = "AP_FiniteHyperElStructure"
            verbose_level = 1

            displacement = "uu"
            level_to_update = 0
            level_of_explicit = 0 

            quadrature_rule_provider = "GE_QRprovider_5" //????

            param_external_load = "bend"
	    param_source = "no_source"
            
            max_nb_iterations = 100
            disp_tolerance = 1.e-8
            
            MODULE AP_ConstitutiveLaw
               concrete_name = "AP_StVenantKirchhoff"
               Young_modulus = $DS_YOUNG
               Poisson_coefficient = $DS_POISSON
            END MODULE AP_ConstitutiveLaw

            MODULE LA_Matrix
               concrete_name = "LA_PelMatrix"
            END MODULE LA_Matrix
            
            MODULE LA_Solver
               concrete_name = "LA_PreconditionedSolver"
               MODULE LA_IterativeSolver
                  concrete_name = "LA_CG_IS"
                  nb_iterations_max = 2000
                  MODULE LA_ConvergenceTest
                     concrete_name = "LA_DefaultConvergenceTest"
                     relative_tolerance =  1.E-07
                     absolute_tolerance = 1.E-20
                  END MODULE LA_ConvergenceTest
                  verbose = false
               END MODULE LA_IterativeSolver
               MODULE LA_Preconditioner
                  concrete_name = "LA_Jacobi_PC"
                  smallest_inverted_item = 1.0e-20
               END MODULE LA_Preconditioner                  
            END MODULE LA_Solver
                        
         END MODULE FE_OneStepIteration#2

      END MODULE list_of_FE_OneStepIteration
      
   END MODULE FE_OneStepIteration
   
END MODULE PEL_Application
