MODULE PEL_Application
   concrete_name = "peldoc"
   $SS_Pelicans = getenv("PELICANSHOME")
   scanned_dir = < "src" >
   ext_filter = < ".cc" >
   include_directories = vector( join( "..", "..", "include" ) )
   format = "html"
   verbose = false
   debug = false
   exclude = < "PEL_assertions.hh" "PEL_assertions.icc" >
   packaging = "description.txt"
   upper = < "PEL_Object" >
END MODULE PEL_Application
